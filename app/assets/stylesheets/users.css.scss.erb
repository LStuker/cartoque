@import 'mixins';
#user_authentication_token { background-color:#fff; }
#authentication_token { font-weight:bold; color:#555; }
#remove-token {
  background-color:#c22; color:#fff; @include gradient(#d44, #c33); text-shadow:none; border:1px solid #c22; padding:2px 4px; margin-left:3px;
  &:hover { @include box-shadow(#ddd, 0, 0, 2px); border-color:#b22; @include gradient(#d74444, #c73333); @include transition(all, .1s); }
}
.users .api { color:#292; font-weight:bold; line-height:12px; padding-bottom:7px; }
.hidden-if-cas.cas { display:none; }
.hidden-if-internal:not(.cas) { display:none; }
