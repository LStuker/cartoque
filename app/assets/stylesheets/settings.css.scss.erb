#settings {
  th { min-width:140px; text-align:left; }
  th, td { border-bottom:1px solid #eee; }
  tr:last-child {
    th, td { border-bottom:0; }
  }
  label.select { position:relative; top:3px; }
}
#edit-visibility {
  table.table { margin-bottom:0; }
  table.table tbody:first-child tr:first-child td { border-top: 0; }
}
