<tr class="fix-on-scroll">
  <th><%= t(:server) %></th>
  <th><%= t(:application_instance) %></th>
  <th>JVM</th>
  <th>Xms / Xmx</th>
  <th>Home</th>
  <th><%= t(:applications) %></th>
</tr>
<% @tomcats_old.each do |tomcat|%>
<tr>
  <td><%= link_to_server_if_exists(tomcat.server).html_safe %></td>
  <td><%= tomcat.tomcat %></td>
  <td><%= tomcat.java %></td>
  <td><%= tomcat.xms %> / <%= tomcat.xmx %></td>
  <td><%= tomcat.home %></td>
  <td><%= tomcat.apps %></td>
</tr>
<% end %>
