<tr class="fix-on-scroll">
  <th class="left">Editeur</th>
  <th class="left">Clé</th>
  <th class="left">Intitulé</th>
  <th>Quantité</th>
  <th>Affectation</th>
  <th>Depuis le</th>
  <th>A renouveler le</th>
  <th class="right">Commentaire</th>
</tr>
<% @licenses.each do |license| %>
<tr>
  <td><%= license.editor %></td>
  <td><%= license.key %></td>
  <td><%= license.title %></td>
  <td class="center"><%= license.quantity %></td>
  <td class="center"><%= license.servers.sort_by(&:name).map{|s| link_to s, s}.join("<br/>").html_safe %></td>
  <td class="center"><%= license.purshased_on %></td>
  <td class="center"><%= license.renewal_on %></td>
  <td><%= license.comment %></td>
</tr>
<% end %>
